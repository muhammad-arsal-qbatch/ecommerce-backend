{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": { "react/react-in-jsx-scope":"off",
  "quotes":["error","single"],
  "semi":[0, "never"],        
  "no-trailing-spaces":"warn",
  "indent-legacy": ["error", 2]
}
}